	"license": "MIT",
	"dependencies": {
		"mapped-replacer": "^1.2.1"
	},
	"devDependencies": {
		"@types/chai": "^4.3.3",
		"@types/mocha": "^9.1.1",
		"chai": "^4.3.6",
		"mocha": "^10.0.0",
		"typescript": "^4.7.4"
	}
